{
  "source": "test/folder_to_monitor",
  "target": "test/target_folder",
  "rules": [
    {
      "match": "stalker",
      "types": [
        "Video",
        "subtitles"
      ],
      "to": "Serie/Stalker",
      "seasonSplit": true
    },
    {
      "match": "Big Bang Theory",
      "to": "Serie/Big Bang Theory"
    },
    {
      "match": "House of cards",
      "to": "Serie/House of Cards"
    },
    {
      "match": "Person of Interest",
      "to": "Serie/Person of Interest"
    },
    {
      "match": "sample",
      "action": "delete",
      "priority": 0
    },
    {
      "size": ">500MB",
      "type": "Video",
      "to": "Films"
    }
  ],
  "types": {
    "Video": {
      "extensions": [
        "avi",
        "mp4",
        "mkv",
        "mov"
      ]
    },
    "music": {
      "extensions": [
        "mp3",
        "wma",
        "ogg"
      ]
    },
    "subtitles": {
      "extensions": [
        "srt",
        "sub"
      ]
    }
  }
}
