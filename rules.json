{
  "source": "test/folder_to_monitor",
  "target": "test/target_folder",
  "rules": [
    {
      "match": "stalker",
      "type": "Serie",
      "to": "Serie/Stalker"
    },
    {
      "match": "Big Bang Theory",
      "type": "Serie",
      "to": "Serie/Big Bang Theory"
    },
    {
      "match": "House of cards",
      "type": "Serie",
      "to": "Serie/House of Cards"
    },
    {
      "match": "Person of Interest",
      "type": "Serie",
      "to": "Serie/Person of Interest"
    },
    {
      "match": "sample",
      "type": "fanta_delete",
      "priority": 60
    },
    {
      "type": "fanta_delete",
      "priority": 0
    },
    {
      "match": "put_in_folders.bat",
      "action": "keep",
      "priority": 100
    },
    {
      "size": ">500MB",
      "type": "Video",
      "to": "Films"
    }
  ],
  "types": {
    "Video": {
      "extensions": [
        "avi",
        "mp4",
        "mkv",
        "mov"
      ]
    },
    "music": {
      "extensions": [
        "mp3",
        "wma",
        "ogg"
      ]
    },
    "subtitles": {
      "extensions": [
        "srt",
        "sub"
      ]
    },
    "serie": {
      "seasonSplit": true,
      "types": [
        "video",
        "subtitles"
      ]
    },
    "fanta_delete": {
      "action": "move",
      "to": "_trash",
      "priority": 0,
      "overwrite": "rename"
    }
  },
  "kodi":{
    "host": "localhost",
    "request_update": true
  }
}
